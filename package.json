{
  "name": "cs-layout",
  "description": "Layout your artboards based on margins set in your layer names. Takes into account the capline and baseline offset for AktivGrotesk too.",
  "version": "1.0.1",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "cs-layout",
    "manifest": "src/manifest.json",
    "main": "cs-layout.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "author": "Seb Bailey <sebastian@clearscore.com>",
  "repository": "https://github.com/sebcglbailey/cs-layout"
}
